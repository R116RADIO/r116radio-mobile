{
  "name": "R116Radio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "REACT_NATIVE_FSOP_TIMEOUT=60000 NODE_ENV=development node node_modules/react-native/local-cli/cli.js start",
    "android-linux": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "react-devtool": "react-devtools",
    "redux-devtool": "remotedev",
    "test:watch": "npm run test -- --watch --maxWorkers=1 --bail",
    "clean": "npm start -- --reset-cache",
    "flow": "flow",
    "flow:check": "flow check",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "immutability-helper": "2.2.0",
    "invariant": "2.2.2",
    "json-stringify-safe": "5.0.1",
    "lessly": "0.2.4",
    "lodash": "4.17.4",
    "moment": "2.17.1",
    "prop-types": "15.5.10",
    "react": "16.0.0-alpha.12",
    "react-native": "0.45.1",
    "react-native-animatable": "1.2.4",
    "react-native-audio-streaming": "2.3.2",
    "react-native-background-timer": "^1.3.0",
    "react-native-fetch-blob": "0.10.8",
    "react-native-image-progress": "1.0.1",
    "react-native-orientation": "3.1.0",
    "react-native-permissions": "1.0.0",
    "react-native-progress": "3.4.0",
    "react-native-router-flux": "4.0.0-beta.21",
    "react-redux": "5.0.6",
    "recompose": "0.24.0",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-logger": "3.0.6",
    "redux-persist": "4.8.3",
    "redux-persist-migrate": "4.1.0",
    "redux-persist-transform-filter": "0.0.15",
    "redux-saga": "0.15.6",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "route-parser": "0.0.5",
    "url-parse": "1.1.9",
    "uuid": "3.1.0",
    "validator": "8.1.0"
  },
  "jest": {
    "preset": "react-native",
    "bail": true,
    "verbose": true,
    "automock": false,
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "moduleNameMapper": {
      "^React$": "<rootDir>/node_modules/react"
    },
    "haste": {
      "defaultPlatform": "ios",
      "platforms": [
        "android",
        "ios",
        "native",
        "windows"
      ],
      "providesModuleNodeModules": [
        "react-native"
      ]
    }
  },
  "rnpm": {
    "assets": [
      "src/fonts/assets"
    ]
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-jest": "20.0.3",
    "babel-plugin-flow-react-proptypes": "5.1.2",
    "babel-plugin-transform-decorators": "6.22.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-jest": "20.0.0",
    "babel-preset-react-native": "1.9.1",
    "babel-preset-react-native-stage-0": "1.0.1",
    "enzyme": "2.8.0",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-jsx-a11y": "1.3.0",
    "eslint-plugin-react": "4.2.3",
    "eslint-plugin-react-native": "1.1.0",
    "flow-bin": "0.53.1",
    "flow-typed": "2.1.5",
    "husky": "0.13.3",
    "jest": "20.0.4",
    "jest-enzyme": "3.5.0",
    "jest-repl": "19.0.2",
    "my-local-ip": "1.0.0",
    "react-addons-test-utils": "15.4.0",
    "react-apollo": "1.4.2",
    "react-devtools": "2.1.3",
    "react-native-mock": "0.3.1",
    "react-test-renderer": "16.0.0-alpha.12",
    "remote-redux-devtools": "0.5.10",
    "remotedev-server": "0.2.2",
    "sc-broker-cluster": "4.0.0"
  }
}
